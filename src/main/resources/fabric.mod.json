{
  "schemaVersion": 1,
  "id": "decorative",
  "version": "${version}",

  "name": "Decorative",
  "description": "Adds many cool new decorative items with 3d models and animations!",
  "authors": [
    "Motschen",
    "TeamMidnightDust"
  ],
  "contact": {
    "homepage": "https://www.midnightdust.eu/",
    "sources": "https://github.com/TeamMidnightDust/Decorative",
    "issues": "https://github.com/TeamMidnightDust/Decorative/issues"
  },

  "license": "MIT",
  "icon": "assets/decorative/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "eu.midnightdust.motschen.decorative.DecorativeMain"
    ],
    "client": [
      "eu.midnightdust.motschen.decorative.DecorativeClient"
    ],
    "modmenu": [
      "eu.midnightdust.motschen.decorative.config.ModMenuIntegration"
    ]
  },

  "mixins": [
    "decorative.mixins.json"
  ],

  "depends": {
    "fabric": "*",
    "autoconfig1u": "*",
    "cloth-config2": "*"
  },
  "suggests": {
    "patchouli": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/jAGnWYHm3r"
      }
    }
